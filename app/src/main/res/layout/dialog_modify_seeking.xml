<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/outside_container"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:id="@+id/outside_view"
        android:layout_width="@dimen/dimen_0dp"
        android:layout_height="@dimen/dimen_0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/seeking_container"
        />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/seeking_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@mipmap/bg_dialog_edit_info"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">
        <com.makeramen.roundedimageview.RoundedImageView
            android:layout_width="@dimen/dimen_0dp"
            android:layout_height="157dp"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:riv_corner_radius_top_left="@dimen/dimen_20dp"
            app:riv_corner_radius_top_right="@dimen/dimen_20dp" />

        <TextView
            android:id="@+id/txt_seeking_define"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_20dp"
            android:fontFamily="@font/cabin_medium"
            android:text="@string/seeking_for"
            android:textColor="@color/color_202323"
            android:textSize="@dimen/dimen_20sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/dialog_close"
            android:layout_width="@dimen/dimen_44dp"
            android:layout_height="@dimen/dimen_44dp"
            android:paddingEnd="@dimen/dimen_22dp"
            android:paddingTop="@dimen/dimen_24dp"
            android:src="@mipmap/icon_black_close_normal"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <RadioGroup
            android:id="@+id/rg_gender_container"
            android:layout_width="@dimen/dimen_0dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txt_seeking_define"
            android:layout_marginHorizontal="@dimen/dimen_30dp"
            android:layout_marginTop="@dimen/dimen_30dp"
            >
            <RadioButton
                android:id="@+id/rb_female"
                android:layout_width="@dimen/dimen_0dp"
                android:layout_height="@dimen/dimen_90dp"
                android:layout_weight="1"
                android:button="@null"
                android:paddingTop="@dimen/dimen_16dp"
                android:text="@string/female"
                android:gravity="center"
                android:fontFamily="@font/cabin_medium"
                android:textColor="@color/color_202323"
                android:textSize="@dimen/dimen_16sp"
                android:background="@drawable/selector_gender_bg"
                />
            <RadioButton
                android:id="@+id/rb_male"
                android:layout_width="@dimen/dimen_0dp"
                android:layout_height="@dimen/dimen_90dp"
                android:layout_weight="1"
                android:button="@null"
                android:paddingTop="@dimen/dimen_16dp"
                android:text="@string/male"
                android:gravity="center"
                android:fontFamily="@font/cabin_medium"
                android:textColor="@color/color_202323"
                android:textSize="@dimen/dimen_16sp"
                android:background="@drawable/selector_gender_bg"
                android:layout_marginHorizontal="@dimen/dimen_22dp"
                />
            <RadioButton
                android:id="@+id/rb_queer"
                android:layout_width="@dimen/dimen_0dp"
                android:layout_height="@dimen/dimen_90dp"
                android:layout_weight="1"
                android:button="@null"
                android:paddingTop="@dimen/dimen_16dp"
                android:text="@string/gender_queer"
                android:gravity="center"
                android:fontFamily="@font/cabin_medium"
                android:textColor="@color/color_202323"
                android:textSize="@dimen/dimen_16sp"
                android:background="@drawable/selector_gender_bg"
                />
        </RadioGroup>


        <TextView
            android:id="@+id/txt_seeking_save"
            android:layout_width="@dimen/dimen_0dp"
            android:layout_height="@dimen/dimen_52dp"
            android:layout_marginHorizontal="@dimen/dimen_28dp"
            android:layout_marginTop="@dimen/dimen_140dp"
            android:layout_marginBottom="@dimen/dimen_31dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:background="@drawable/selector_button_isenable"
            android:enabled="true"
            android:fontFamily="@font/cabin_semibold"
            android:gravity="center"
            android:orientation="horizontal"
            android:text="@string/save"
            android:textAllCaps="true"
            android:textColor="@color/white"
            android:textSize="@dimen/dimen_16sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/rg_gender_container" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>